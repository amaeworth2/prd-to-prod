# Google Maps API Key (client-side, restricted by HTTP referrer in Google Cloud Console)
NEXT_PUBLIC_GMAK=your_google_maps_api_key_here

# Stadium Location (public)
NEXT_PUBLIC_STADIUM_NAME="Mercedes-Benz Stadium"
NEXT_PUBLIC_STADIUM_LAT="33.754542"
NEXT_PUBLIC_STADIUM_LNG="-84.402492"

# MARTA API Keys (server-side only)
MARTA_API_KEY=your_marta_api_key_here
MARTA_TRAIN_API_KEY=your_marta_train_api_key_here

# Use mock MARTA data for demonstration
USE_MOCK_MARTA_DATA=false
